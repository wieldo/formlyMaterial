{
  "name": "wieldoformlymaterial",
  "version": "0.1.3",
  "description": "Wieldo formly Material to build forms with angular-formly and angular material design",
  "author": "Wieldo <office@ster-project.com>",
  "contributors": [
    "Åšcibor Rudnicki <scibor.rudnicki@ster-project.com>"
  ],
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "main": "./lib/wieldo-formly-material.min.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wieldo/formlyMaterial.git"
  },
  "scripts": {
    "build:dev": "webpack --config tools/webpack.config.js --progress --set-env-NODE_ENV=dev",
